{
  "name": "@frameless/strapi-plugin-nl-government-fields",
  "version": "0.0.0",
  "description": "A Strapi v5 plugin that provides reusable custom fields for Dutch government and public-sector product metadata.",
  "keywords": [],
  "private": false,
  "strapi": {
    "name": "nl-government-fields",
    "description": "A Strapi v5 plugin that provides reusable custom fields for Dutch government and public-sector product metadata.",
    "kind": "plugin",
    "displayName": "Dutch government fields"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi-v5-extensions.git",
    "directory": "packages/strapi-plugin-nl-government-fields"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "author": {
    "name": "@Frameless"
  },
  "license": "EUPL-1.2",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "peerDependencies": {
    "@strapi/design-system": ">=2.0.0",
    "@strapi/icons": ">=2.0.0",
    "@strapi/strapi": ">=5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=22.0.0 <25",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@frameless/upl": "workspace:*",
    "@strapi/utils": "5.36.1",
    "react-intl": "8.1.3",
    "react-select": "5.10.2",
    "react-window": "2.2.7"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@frameless/eslint-config": "workspace:*",
    "@frameless/typescript-config": "workspace:*",
    "@strapi/admin": "5.36.1",
    "@strapi/design-system": "2.1.2",
    "@strapi/icons": "2.1.2",
    "@strapi/sdk-plugin": "5.4.0",
    "@strapi/typescript-utils": "5.36.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "29.5.12",
    "@types/node": "24.10.9",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "rimraf": "6.1.3",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint-build": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test:ts:front": "run -T tsc -p tsconfig.json",
    "test:ts:back": "run -T tsc -p tsconfig.server.json",
    "clean": "rimraf .next .strapi dist"
  }
}
