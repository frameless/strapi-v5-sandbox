{
  "name": "@frameless/preview-button",
  "version": "1.2.0",
  "description": "A Strapi plugin that enable preview button",
  "private": false,
  "keywords": [],
  "strapi": {
    "name": "preview-button",
    "description": "Description of Preview Button plugin",
    "kind": "plugin",
    "displayName": "Preview Button"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi.git",
    "directory": "packages/preview-button"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "dependencies": {
    "@strapi/design-system": "2.1.2",
    "@strapi/icons": "2.1.2",
    "@strapi/utils": "5.33.4",
    "immer": "11.1.3",
    "react-intl": "8.1.1",
    "react-redux": "9.2.0",
    "styled-components": "6.3.8"
  },
  "author": {
    "name": "@frameless"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "EUPL-1.2",
  "peerDependencies": {
    "@strapi/design-system": ">=2.0.0",
    "@strapi/icons": ">=2.0.0",
    "@strapi/strapi": ">=5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@frameless/eslint-config": "workspace:*",
    "@frameless/typescript-config": "workspace:*",
    "@strapi/sdk-plugin": "5.4.0",
    "@strapi/strapi": "5.33.4",
    "@strapi/typescript-utils": "5.33.4",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "29.5.12",
    "@types/koa": "3.0.1",
    "@types/node": "24.10.9",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@types/styled-components": "5.1.36",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint-build": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test:ts:front": "run -T tsc -p tsconfig.json",
    "test:ts:back": "run -T tsc -p tsconfig.server.json",
    "clean": "rimraf .next .strapi dist"
  }
}
