{
  "name": "@frameless/strapi-plugin-language",
  "version": "0.0.0",
  "description": "A strapi custom field for selecting a language attribute",
  "keywords": [],
  "private": false,
  "strapi": {
    "name": "language",
    "description": "A strapi custom field for selecting a language attribute",
    "kind": "plugin",
    "displayName": "Select uniform product name"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi-v5-extensions.git",
    "directory": "packages/strapi-plugin-language"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "author": {
    "name": "@Frameless"
  },
  "license": "EUPL-1.2",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "peerDependencies": {
    "@strapi/design-system": ">=2.0.0",
    "@strapi/icons": ">=2.0.0",
    "@strapi/strapi": ">=5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=22.0.0 <25",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "4.2.0",
    "@strapi/utils": "5.33.4",
    "react-intl": "8.1.1"
  },
  "devDependencies": {
    "@frameless/eslint-config": "workspace:*",
    "@frameless/typescript-config": "workspace:*",
    "@strapi/design-system": "2.1.2",
    "@strapi/icons": "2.1.2",
    "@strapi/sdk-plugin": "5.4.0",
    "@strapi/typescript-utils": "5.33.4",
    "@types/node": "24.10.9",
    "rimraf": "6.1.2"
  },
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint-build": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test:ts:front": "run -T tsc -p tsconfig.json",
    "test:ts:back": "run -T tsc -p tsconfig.server.json",
    "clean": "rimraf .next .strapi dist"
  }
}
