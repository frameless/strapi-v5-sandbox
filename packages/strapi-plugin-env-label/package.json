{
  "name": "@frameless/strapi-plugin-env-label",
  "version": "1.0.0",
  "description": "A Strapi plugin designed to visually represent the current environment.",
  "keywords": [],
  "private": false,
  "strapi": {
    "name": "env-label",
    "description": "A helpful plugin for displaying the current environment.",
    "kind": "plugin",
    "displayName": "Display the current environment"
  },
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/strapi-v5-extensions.git",
    "directory": "packages/strapi-plugin-env-label"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "author": {
    "name": "@Frameless"
  },
  "license": "EUPL-1.2",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "peerDependencies": {
    "@strapi/design-system": ">=2.0.0",
    "@strapi/icons": ">=2.0.0",
    "@strapi/strapi": ">=5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=22.0.0 <25",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@strapi/helper-plugin": "4.25.23",
    "@utrecht/component-library-css": "9.0.0",
    "@utrecht/component-library-react": "12.0.0",
    "@utrecht/design-tokens": "5.0.1",
    "immer": "11.1.3",
    "react-intl": "8.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@frameless/eslint-config": "workspace:*",
    "@frameless/typescript-config": "workspace:*",
    "@strapi/design-system": "2.1.2",
    "@strapi/icons": "2.1.2",
    "@strapi/sdk-plugin": "5.4.0",
    "@strapi/strapi": "5.33.4",
    "@strapi/typescript-utils": "5.33.4",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "29.5.12",
    "@types/node": "24.10.9",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@types/styled-components": "5.1.36",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "koa": "3.1.1",
    "rimraf": "6.1.2",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint-build": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test:ts:front": "run -T tsc -p tsconfig.json",
    "test:ts:back": "run -T tsc -p tsconfig.server.json",
    "clean": "rimraf .next .strapi dist"
  }
}
