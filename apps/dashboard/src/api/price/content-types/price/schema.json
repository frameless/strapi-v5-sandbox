{
  "kind": "collectionType",
  "collectionName": "prices",
  "info": {
    "singularName": "price",
    "pluralName": "prices",
    "displayName": "price"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "component",
      "component": "components.price",
      "repeatable": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "price"
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-regenerate": true
      }
    }
  }
}
